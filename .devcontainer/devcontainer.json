{
  "image": "docker.io/keybrl/devkits:latest",
  "mounts": [{
    "source": "/var/run/docker.sock",
    "target": "/var/run/docker.sock",
    "type": "bind"
  }, {
    "source": "${localEnv:HOME}/.ssh",
    "target": "/root/.ssh",
    "type": "bind"
  }],
  "customizations": {
    "vscode": {
      "extensions": [
        "EditorConfig.EditorConfig"
      ]
    }
  }
}
